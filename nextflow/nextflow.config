process {
    cpus = 4
    cleanup = true
}

timeline {
  enabled = true
  timeline.overwrite = true
  file = "$launchDir/log_nextflow/nf_timeline.html"
}

report {
  enabled = true
  report.overwrite = true
  file = "$launchDir/log_nextflow/nf_report.html"
}

profiles {
  conda {
    conda.enabled = true
  }

  docker {
    docker.enabled = true
  }
}